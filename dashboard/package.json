{
  "name": "@usex/nest-shield-dashboard",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "description": "A dashboard for managing NestJS Shield rules and policies.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ali-master/nest-shield.git",
    "directory": "dashboard"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,mdx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js}\"",
    "lint:fix": "pnpm run lint --fix",
    "check-types": "tsc --noEmit --pretty",
    "prepublish:next": "pnpm run build",
    "publish:next": "pnpm publish --access public --tag next",
    "prepublish:npm": "pnpm run build",
    "publish:npm": "pnpm publish --access public",
    "prerelease": "pnpm run build",
    "release": "release-it",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "commit": "cz",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv -c production -- drizzle-kit migrate",
    "db:studio": "dotenv -c production -- drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:demo": "tsx scripts/seed.ts --demo",
    "db:seed:interactive": "tsx scripts/seed.ts --interactive",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:setup": "npm run db:migrate && npm run db:seed:demo"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.9.1",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^9.29.0",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-table": "^8.21.3",
    "argon2": "^0.43.0",
    "chalk": "^5.4.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "critters": "^0.0.25",
    "drizzle-orm": "^0.44.2",
    "framer-motion": "^12.18.1",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.516.0",
    "mysql2": "^3.14.1",
    "next": "^15.3.3",
    "next-auth": "^4.24.11",
    "next-intl": "^4.1.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.3",
    "ora": "^8.2.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.58.0",
    "resend": "^4.6.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.5",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "ua-parser-js": "^2.0.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@next/bundle-analyzer": "^15.3.3",
    "@next/eslint-plugin-next": "^15.3.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@spotlightjs/spotlight": "^3.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/speakeasy": "^2.0.10",
    "@types/ua-parser-js": "^0.7.39",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/expect": "^3.2.3",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.7",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^7.5.3",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "next-svgr": "^0.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.5",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3",
    "vitest-fail-on-console": "^0.7.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "release": {
    "branches": [
      {
        "name": "beta",
        "prerelease": true,
        "channel": "beta"
      },
      {
        "name": "next",
        "prerelease": true,
        "channel": "next"
      },
      {
        "name": "master",
        "prerelease": true,
        "channel": "next"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
